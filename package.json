{
    "name": "workshop-studio",
    "description": "Your Personal Productivity & Maker's Hub - AI Assistant, Project Management, and Tools Inventory",
    "private": true,
    "version": "2.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "build:full": "npm run build && xcopy /Y server.js dist\\ && xcopy /Y web.config dist\\ && xcopy /Y package.json dist\\ && xcopy /Y ecosystem.config.cjs dist\\ && xcopy /E /Y utils dist\\utils\\",
        "lint": "eslint .",
        "preview": "vite preview",
        "server": "node server.js",
        "start:all": "npm run server & npm run dev",
        "convert-pdfs": "node convert-pdfs-to-images.js",
        "version:build": "PowerShell -ExecutionPolicy Bypass -File ./increment-version.ps1 -Type build",
        "version:patch": "PowerShell -ExecutionPolicy Bypass -File ./increment-version.ps1 -Type patch",
        "version:minor": "PowerShell -ExecutionPolicy Bypass -File ./increment-version.ps1 -Type minor",
        "version:major": "PowerShell -ExecutionPolicy Bypass -File ./increment-version.ps1 -Type major",
        "deploy": "PowerShell -ExecutionPolicy Bypass -File ./deploy-with-version.ps1",
        "deploy:patch": "PowerShell -ExecutionPolicy Bypass -File ./deploy-with-version.ps1 -VersionType patch",
        "deploy:minor": "PowerShell -ExecutionPolicy Bypass -File ./deploy-with-version.ps1 -VersionType minor"
    },
    "dependencies": {
        "@azure/msal-browser": "^4.25.0",
        "@azure/msal-react": "^3.0.20",
        "@dnd-kit/core": "^6.3.1",
        "@dnd-kit/sortable": "^10.0.0",
        "@dnd-kit/utilities": "^3.2.2",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@mui/icons-material": "^7.3.4",
        "@mui/material": "^7.3.4",
        "@mui/system": "^7.3.3",
        "canvas": "^3.2.0",
        "cors": "^2.8.5",
        "express": "^5.1.0",
        "html2canvas": "^1.4.1",
        "lucide-react": "^0.544.0",
        "mysql2": "^3.15.2",
        "node-fetch": "^2.7.0",
        "openai": "^6.3.0",
        "pdf-lib": "^1.17.1",
        "pdf2pic": "^3.2.0",
        "pdfjs-dist": "^5.4.296",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-markdown": "^10.1.0",
        "react-pdf": "^10.2.0",
        "rehype-raw": "^7.0.0",
        "sharp": "^0.34.5",
        "xlsx": "^0.18.5"
    },
    "devDependencies": {
        "@eslint/js": "^9.36.0",
        "@tailwindcss/postcss": "^4.1.14",
        "@types/node": "^24.6.0",
        "@types/react": "^19.1.16",
        "@types/react-dom": "^19.1.9",
        "@vitejs/plugin-react": "^5.0.4",
        "autoprefixer": "^10.4.21",
        "eslint": "^9.36.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.22",
        "globals": "^16.4.0",
        "postcss": "^8.5.6",
        "tailwindcss": "^4.1.14",
        "typescript": "~5.9.3",
        "typescript-eslint": "^8.45.0",
        "vite": "^7.1.7"
    }
}
